[user]
	email = zhangfangjie@meituan.com
	name = zhangfangjie
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	last = log -1 HEAD
	visual = gitk
	amend = commit --amend -C HEAD
	ls = log --pretty=format:'%C(yellow)%h %C(blue)%ad %C(red)%d %C(reset)%s %C(green) [%cn]' --decorate --date=short
[color]
	diff = auto
	status = auto
	branch = auto
[merge]
	tool = kdiff3
[meregtool "kdiff3"]
	path = /usr/bin/kdiff3
